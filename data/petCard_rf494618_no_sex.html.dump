$(document).on('submit',"#form-registration",function(e) {
    e.preventDefault();    
    $.ajax({    
        type: $("#form-registration").attr('method'),
        url: $("#form-registration").attr('action'),
        data: $("#form-registration").serialize(),
        dataType: 'json',
        success: function(msg){
            if (msg.status == 'success') {
                location.href = msg.location;
            } 
            if (msg.status == 'error') {
                for (var key in msg.errors) {
                  $(".field-fastsignupform-"+key+" .error").html(msg.errors[key][0]);
                }
            } 
        }
    });
});
$(document).on('click','#showRegFieldset',function(e) {
  e.preventDefault();
  $("#signupFieldset").show();
  $("#loginFieldset").hide();
});
$(document).on('click','#showLogFieldset',function(e) {
  e.preventDefault();
  $("#signupFieldset").hide();
  $("#loginFieldset").show();
});
$("#sendComplainFormBtn").on('click',function() {
    $("#complainForm .error").remove();
    if($("#complainMes").val().length<3){
      $("#complainMes").after('<div class="error">Сообщите подробности</div>'); 
    } else {
      $.ajax({    
        type: $("#complainForm").attr('method'),
        url: $("#complainForm").attr('action'),
        data: $("#complainForm").serialize(),
        dataType: 'json',
        success: function(msg){
            if (msg.status == 'success') {
                $("#sendComplainFormBtn").remove();
                $(".modal-header h3").html(msg.title);
                $("#complainForm").html(msg.text);
            } else {
                console.log(msg);
            }
        }
      });
  }
})
