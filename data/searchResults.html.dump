    <script>
jQuery(window).on('load', function () {

    $(document).on('click', "#applyBanAddPet", function(e) {
        e.preventDefault();
        sendBanAddPetBtn('apply');
    });
    $(document).on('click', "#cancelBanAddPet", function(e) {
        e.preventDefault();
        $("#modalBanAddPet").modal('hide');
        sendBanAddPetBtn('cancel');
    });

    function sendBanAddPetBtn(btn){
        $.ajax({
            url: '/ajax/ban-add-pet',
            type: 'post',
            data: 'btn='+btn,
            dataType: 'json',
            success: function(msg){
                if(btn=='apply'){
                    location.href = '/form-add/first-step';
                }
            }
        });
    }

    isShowed = false;
    function eventListenerScroll() {
        const body = document.body,
              html = document.documentElement;
        const height = Math.max(body.scrollHeight, body.offsetHeight,html.clientHeight, html.scrollHeight, html.offsetHeight);
        if (!isShowed && height * 0.4 < window.scrollY) {
            $("#modalBanAddPet").modal('show');
            isShowed = true;
        }
    }

    $(window).on('scroll', eventListenerScroll);
});</script>
